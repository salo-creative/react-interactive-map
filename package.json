{
  "name": "@salo/interactive-map",
  "version": "3.0.0",
  "description": "An SVG interactive map",
  "main": "./index.js",
  "sideEffects": false,
  "scripts": {
    "start": "start-storybook -p 6002 -s ./public",
    "bundle-modules": "rm -rf dist/ && babel --copy-files src --out-dir dist",
    "bundle-watch": "babel --copy-files src --watch --out-dir dist",
    "bundle": "npm run bundle-modules",
    "demo": "npm run test:output -u && npm start",
    "lint": "eslint ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage --maxWorkers=10",
    "release": "chmod +x ./ci/release.sh && ./ci/release.sh",
    "npm-publish": "npm-publish ./dist",
    "build-storybook": "build-storybook -s ./public -o .out",
    "deploy-storybook": "storybook-to-ghpages",
    "test:output": "jest -u --json --outputFile=.storybook/jest-test-results.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salo-creative/react-interactive-map.git"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test-setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/_story.js",
      "!src/**/*.story.js",
      "!src/helpers/globalStyles/*.js"
    ],
    "notify": true,
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "globals": {
      "webpackVars": {
        "ENV": "test"
      }
    },
    "verbose": true,
    "testURL": "http://localhost:6001/"
  },
  "keywords": [
    "React",
    "Styled",
    "Components",
    "Storybook",
    "Jest"
  ],
  "author": "Rich Comber",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/salo-creative/react-interactive-map/issues"
  },
  "homepage": "https://github.com/salo-creative/react-interactive-map#readme",
  "devDependencies": {
    "@babel/cli": "~7.6.4",
    "@babel/core": "~7.6.4",
    "@babel/plugin-proposal-class-properties": "~7.5.5",
    "@babel/plugin-syntax-decorators": "~7.2.0",
    "@babel/plugin-syntax-dynamic-import": "~7.2.0",
    "@babel/plugin-transform-runtime": "~7.6.2",
    "@babel/preset-env": "~7.6.3",
    "@babel/preset-react": "~7.6.3",
    "@salo/core-ui": "^0.11.19",
    "@salo/icons": "~1.0.13",
    "@storybook/addon-actions": "~5.2.4",
    "@storybook/addon-info": "~5.2.4",
    "@storybook/addon-jest": "~5.2.4",
    "@storybook/addon-knobs": "~5.2.4",
    "@storybook/addon-links": "~5.2.4",
    "@storybook/addon-notes": "~5.2.4",
    "@storybook/addon-options": "~5.2.4",
    "@storybook/addons": "~5.2.4",
    "@storybook/cli": "~5.2.4",
    "@storybook/react": "~5.2.4",
    "@storybook/storybook-deployer": "~2.8.1",
    "@testing-library/jest-dom": "~4.1.2",
    "@testing-library/react": "~9.3.0",
    "@types/react": "~16.9.9",
    "babel-eslint": "~10.0.3",
    "babel-loader": "~8.0.6",
    "babel-plugin-lodash": "~3.3.4",
    "babel-plugin-react-transform": "~3.0.0",
    "babel-plugin-styled-components": "~1.10.6",
    "circular-dependency-plugin": "~5.2.0",
    "css-loader": "~3.2.0",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.15.1",
    "eslint": "~6.5.1",
    "eslint-config-airbnb": "~18.0.1",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-plugin-import": "~2.18.2",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-react": "~7.16.0",
    "eslint-plugin-react-hooks": "~2.1.2",
    "file-loader": "~4.2.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~24.9.0",
    "jest-styled-components": "~6.3.3",
    "lodash": "~4.17.11",
    "node-sass": "~4.13.0",
    "npm-cli-login": "~0.1.1",
    "polished": "~3.4.1",
    "prop-types": "~15.5.10",
    "raw-loader": "~0.5.1",
    "react": "~16.8.4",
    "react-dom": "~16.8.4",
    "react-router-dom": "~4.2.2",
    "sass-loader": "~6.0.6",
    "style-loader": "~1.0.0",
    "styled-components": "~4.4.0",
    "url-loader": "~2.2.0",
    "webpack": "~4.41.2"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.2.0",
    "lodash": "~4.17.11",
    "polished": "~3.4.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.6.3"
  }
}
